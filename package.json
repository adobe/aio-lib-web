{
  "name": "@adobe/aio-lib-web",
  "version": "5.0.0",
  "description": "Utility tooling library to build and deploy Adobe I/O Project Firefly app static sites to CDN",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "scripts": "scripts"
  },
  "author": "Adobe Inc.",
  "license": "Apache-2.0",
